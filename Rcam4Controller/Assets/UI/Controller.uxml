                    <Bindings>
                        <engine:DataBinding property="value" data-source-path="Knob0" binding-mode="ToSource" />
                    </Bindings>
                </VJUITK.VJKnob>
                <VJUITK.VJKnob label="Flame" high-value="1">
                    <Bindings>
                        <engine:DataBinding property="value" data-source-path="Knob1" binding-mode="ToSource" />
